                "responses": [
                    {
                        "code": 200,
                        "description": "The application service indicates that this room alias exists. The\napplication service MUST have created a room and associated it with\nthe queried room alias using the client-server API. Additional\ninformation about the room such as its name and topic can be set\nbefore responding.",
                        "example": "{}"
